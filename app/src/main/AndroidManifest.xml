<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arik.pupukapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
                android:name=".PupukActivity"
                android:label="@string/layout_pupuk" />
        <activity
                android:name=".RekomendasiActivity"
                android:label="@string/layout_acuan" />
        <activity
                android:name=".ProsesActivity"
                android:label="@string/layout_proses_optimasi" />
        <activity
                android:name=".PerhitunganActivity"
                android:label="@string/layout_perhitungan" />
        <activity
                android:name=".TambahPupukActivity"
                android:label="@string/layout_tambah" />
        <activity
                android:name=".EditPupukActivity"
                android:label="@string/layout_edit" />
        <activity
                android:name=".HapusPupukActivity"
                android:label="@string/layout_hapus" />
        <activity
                android:name=".HasilProsesActivity"
                android:label="@string/layout_hasil_proses" />

    </application>

</manifest>