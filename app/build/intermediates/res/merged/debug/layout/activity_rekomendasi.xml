<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TableLayout
        android:id="@+id/acuantable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp">

        <TableRow>

            <TextView
                android:padding="4dp"
                android:text="ID" />

            <TextView
                android:padding="4dp"
                android:text="Kab" />

            <TextView
                android:padding="4dp"
                android:text="Kec" />

            <TextView
                android:padding="4dp"
                android:text="N" />

            <TextView
                android:padding="4dp"
                android:text="P" />

            <TextView
                android:padding="4dp"
                android:text="K" />
        </TableRow>

    </TableLayout>

</ScrollView>
